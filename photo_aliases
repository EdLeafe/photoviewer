alias pausephoto='kill -SIGTSTP `cat /home/pi/projects/photoviewer/photo.pid`'
alias resumephoto='kill -SIGCONT `cat /home/pi/projects/photoviewer/photo.pid`'
alias nextphoto='kill -SIGTRAP `cat /home/pi/projects/photoviewer/photo.pid`'
alias readcfg='kill -SIGHUP `cat /home/pi/projects/photoviewer/photo.pid`'
alias hostsync='kill -SIGURG `cat /home/pi/projects/photoviewer/photo.pid`'
