alias pausephoto='kill -SIGTSTP `cat /home/ed/projects/photoviewer/photo.pid`'
alias resumephoto='kill -SIGCONT `cat /home/ed/projects/photoviewer/photo.pid`'
alias nextphoto='kill -SIGTRAP `cat /home/ed/projects/photoviewer/photo.pid`'
alias readcfg='kill -SIGHUP `cat /home/ed/projects/photoviewer/photo.pid`'
alias hostsync='kill -SIGURG `cat /home/ed/projects/photoviewer/photo.pid`'
alias cdp='cd ~/projects/photoviewer'
